<!-- views/index.handlebars -->
<!doctype html>
<html>
<head>
    <title>Node Authentication</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css"> <!-- load bootstrap css -->
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css"> <!-- load fontawesome -->
    <style>
        <!--body        { padding-top:80px; } -->
    </style>
</head>
<body>
    
    
    <!-- Start Gallery 1-1 -->
    <section class="content-block gallery-1">
        
        <div class="container">
        
          

            
            <div class="row">
                <div id="isotope-gallery-container">
                    {{#each stuff}}
                    <div class="col-md-3 col-sm-6 col-xs-12 gallery-item-wrapper artwork creative">
                        <div class="gallery-item">                            <div class="gallery-thumb">

                                <img src="https://maps.googleapis.com/maps/api/streetview?size=600x300&location={{this.ADDRESS}}{{this.City}}{{this.State}}{{this.Zip}}&key=AIzaSyDyEo8dMx4Z9q4VxaJfDhM5u4yulYX5afo" class="img-responsive" alt="1st gallery Thumb">
                                <div class="image-overlay"></div>
                                <a href="http://placehold.it/800x600" rel="prettyPhoto[pp_gal]" class="gallery-zoom"><i class="fa fa-eye"></i></a>
                                <a href="#" class="gallery-link"><i class="fa fa-link"></i></a>
                            </div>
                            <div class="gallery-details">
                                <div class="editContent">
                                    <h5>{{this.ADDRESS}}</h5>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- /.gallery-item-wrapper -->
                    {{/each}}

                    
                </div>
                <!-- /.isotope-gallery-container -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container -->
    </section>
    <!--// End Gallery 1-1 -->
    


</body>
</html>